{
  "name": "flight-logger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:express": "ts-node --project tsconfig.server.json src/server.ts",
    "build:express": "next build",
    "start:express": "cross-env NODE_ENV=production & ts-node --project tsconfig.server.json src/server.ts",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist && rimraf .next && rimraf build",
    "clean:cache": "rimraf node_modules/.cache"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.4.2",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lucide-react": "^0.427.0",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.2",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.20",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.2",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
